{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "paths": {
      "react": ["./node_modules/preact/compat/"],
      "react-dom": ["./node_modules/preact/compat/"]
    },
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "files": ["node_modules/vite/client.d.ts"],
  "include": ["src"]
}
